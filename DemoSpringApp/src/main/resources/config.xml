<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<!-- 
<bean id="Objectname"  class="javaclassname including package">
	<property name="variablename"  value="val to be given" />
</bean>
 -->
 <!-- Setter DI -->
 <bean id="stdObj"  class="com.fw.beans.Student">
 	<property name="rollno"  value="1001" />
 	<property name="sname"  value="Sahasra" />
 	<property name="course"  value="JAVA" />
 	<property name="fees"  value="15000.00" />
 </bean>
 <!-- Constructor DI -->
<bean id="empObj"  class="com.fw.beans.Employee">
	<constructor-arg name="empno"  value="121"/>
	<constructor-arg name="ename"  value="Venugopal"/>
	<constructor-arg name="job"  value="Corporate Trainer"/>
	<constructor-arg name="sal"  value="50000.00"/>
</bean>
</beans>