<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:context="http://www.springframework.org/schema/context"
    xmlns:mvc="http://www.springframework.org/schema/mvc"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />
	
	<bean id="course1"  class="com.spring.autowireing.Course">
   		<property name="cname"  value="Java"/>
   		<property name="fees"  value="15000.00"/>
   		<property name="duration"  value="6 Months"/>
   </bean>
   
   	<bean id="course2"  class="com.spring.autowireing.Course">
   		<property name="cname"  value="dotnet"/>
   		<property name="fees"  value="15000.00"/>
   		<property name="duration"  value="6 Months"/>
   </bean>
   
   <bean id="stdObj"  class="com.spring.autowireing.Student">
   		<property name="rollno"  value="1001"/>
   		<property name="sname"  value="Praveen"/>   		
   </bean>
	
</beans>